@page

@model IndexModel
@{
    ViewData["Title"] = "Home page";
}


<section class="featured">
	<div class="container">
		<h1>Featured Products</h1>
		<div class="product-container">
			@foreach (var product in Model.Products.Take(4))
			{
				<partial name="_DeviceTilePartial" model="product" />
			}

		</div>
	</div>
</section>



